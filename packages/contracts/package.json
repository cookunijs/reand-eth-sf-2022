{
  "name": "contracts",
  "version": "0.1.0",
  "description": "",
  "main": "contracts/Reand.sol",
  "author": "Cook",
  "private": true,
  "scripts": {
    "build": "hardhat compile --config ./hardhat.config.ts",
    "test": "hardhat test --config ./hardhat.config.ts",
    "coverage": "hardhat coverage --config ./hardhat.config.ts",
    "lint:check": "yarn lint:check:format && yarn lint:check:solhint && yarn lint:check:eslint",
    "lint:check:format": "prettier --check **.{sol,js,ts}",
    "lint:check:solhint": "yarn build && solhint --config ./configs/.solhint.json --ignore-path ./configs/.solhintignore contracts/**/*.sol",
    "lint:check:eslint": "eslint . --ext js,ts",
    "lint:fix": "yarn lint:fix:format && yarn lint:fix:eslint",
    "lint:fix:format": "prettier --write **.{sol,js,ts}",
    "lint:fix:eslint": "eslint --fix . --ext js,ts",
    "size-contracts": "hardhat size-contracts",
    "gas-report": "env-cmd -f env/.env.local yarn test",
    "typechain": "yarn hardhat typechain",
    "localchain": "yarn hardhat node",
    "deploy-contract-to-localchain": "env-cmd -f env/.env.local hardhat run scripts/99_batchMigration.ts --network localhost",
    "deploy-contract-to-mainnet": "env-cmd -f env/.env.mainnet hardhat run scripts/99_batchMigration.ts --network mainnet",
    "deploy-contract-to-goerli": "env-cmd -f env/.env.goerli hardhat run scripts/99_batchMigration.ts --network goerli",
    "deploy-contract-to-matic": "env-cmd -f env/.env.matic hardhat run scripts/99_batchMigration.ts --network matic",
    "deploy-contract-to-matic_test": "env-cmd -f env/.env.matic_test hardhat run scripts/1_deploy.ts --network matic_test",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.sol": "prettier --write",
    "*.js": "prettier --write",
    "*.ts": "prettier --write"
  },
  "dependencies": {
    "@appliedzkp/semaphore-contracts": "0.10.0",
    "@openzeppelin/contracts": "4.7.3",
    "@openzeppelin/contracts-upgradeable": "4.7.3",
    "@worldcoin/world-id-contracts": "0.1.1",
    "@zk-kit/identity": "1.4.1",
    "@zk-kit/protocols": "1.11.1",
    "circomlibjs": "0.1.7",
    "env-cmd": "^10.1.0",
    "keccak256": "^1.0.6",
    "merkletreejs": "^0.2.31",
    "on-chain-verification-test": "Rahat-ch/PolygonID-AgeCredential-SBT-Tutorial",
    "semaphore": "1.1.0"
  },
  "devDependencies": {
    "@nomiclabs/hardhat-ethers": "^2.0.0",
    "@nomiclabs/hardhat-etherscan": "^3.0.0",
    "@nomiclabs/hardhat-waffle": "^2.0.0",
    "@typechain/ethers-v5": "^7.0.1",
    "@typechain/hardhat": "^2.3.0",
    "@types/chai": "^4.2.21",
    "@types/mocha": "^9.0.0",
    "@types/node": "^12.0.0",
    "@typescript-eslint/eslint-plugin": "^5.9.1",
    "@typescript-eslint/parser": "^5.9.1",
    "chai": "^4.2.0",
    "dotenv": "^10.0.0",
    "eslint": "^8.6.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-n": "^15.2.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "ethereum-waffle": "^3.0.0",
    "ethers": "^5.6.2",
    "hardhat": "2.12.2",
    "hardhat-contract-sizer": "^2.5.1",
    "hardhat-dependency-compiler": "1.1.3",
    "hardhat-deploy": "^0.11.2",
    "hardhat-gas-reporter": "1.0.9",
    "husky": "7.0.4",
    "lint-staged": ">=10",
    "patch-package": "6.5.0",
    "prettier": "^2.5.1",
    "prettier-plugin-solidity": "^1.0.0-beta.19",
    "solhint": "^3.3.6",
    "solidity-coverage": "^0.7.20",
    "ts-node": "^10.1.0",
    "typechain": "^8.0.0",
    "typescript": "^4.5.2"
  }
}
