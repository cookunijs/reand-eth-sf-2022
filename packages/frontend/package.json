{
  "name": "frontend",
  "version": "0.1.0",
  "author": "cookunijs",
  "private": true,
  "scripts": {
    "clean": "rm -rf .next && rm -rf dist",
    "dev": "env-cmd -f src/env/.env.local next dev -p 3000",
    "build": "yarn clean && next build",
    "set:db": "env-cmd -f src/env/.env.local ts-node --project ./tsconfig.server.json -r tsconfig-paths/register scripts/setDBVars",
    "set:algolia": "env-cmd -f src/env/.env.local ts-node --project ./tsconfig.server.json -r tsconfig-paths/register scripts/setAlgolia",
    "start": "next start",
    "start:local": "env-cmd -f src/env/.env.local next start",
    "start:dev": "env-cmd -f src/env/.env.dev next start",
    "start:staging": "env-cmd -f src/env/.env.staging next start",
    "start:prod": "env-cmd -f env/.src/env.prod next start",
    "lint": "next lint",
    "gcloud:build": "gcloud builds submit --tag gcr.io/$GCLOUD_PROJECT/renderer --project $GCLOUD_PROJECT --timeout=3600s",
    "gcloud:build:dev": "GCLOUD_PROJECT=dev env-cmd -f src/env/.env.dev yarn gcloud:build",
    "gcloud:build:staging": "GCLOUD_PROJECT=staging env-cmd -f src/env/.env.staging yarn gcloud:build",
    "gcloud:build:prod": "GCLOUD_PROJECT=prod env-cmd -f src/env/.env.prod yarn gcloud:build",
    "gcloud:run": "gcloud run deploy renderer --image gcr.io/$GCLOUD_PROJECT/renderer --project $GCLOUD_PROJECT --platform managed --region asia-northeast1 --memory 4Gi --cpu 1 --port 3000 --allow-unauthenticated --flags-file flags-file.yaml",
    "gcloud:run:dev": "cp flags/flags-file.dev.yaml flags-file.yaml && GCLOUD_PROJECT=dev env-cmd -f src/env/.env.dev yarn gcloud:run && rm -rf flags-file.yaml",
    "gcloud:run:staging": "cp flags/flags-file.staging.yaml flags-file.yaml && GCLOUD_PROJECT=staging env-cmd -f src/env/.env.staging yarn gcloud:run && rm -rf flags-file.yaml",
    "gcloud:run:prod": "cp flags/flags-file.prod.yaml flags-file.yaml &&GCLOUD_PROJECT=prod env-cmd -f src/env/.env.prod yarn gcloud:run && rm -rf flags-file.yaml",
    "deploy:dev": "yarn gcloud:build:dev && yarn gcloud:run:dev",
    "deploy:staging": "yarn gcloud:build:staging && yarn gcloud:run:staging",
    "deploy:prod": "yarn gcloud:build:prod && yarn gcloud:run:prod",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public"
  },
  "dependencies": {
    "@algolia/autocomplete-core": "1.7.1",
    "@algolia/autocomplete-preset-algolia": "1.7.1",
